# Production Environment Variables
# Copy this file to .env.production for production builds

# Firebase Configuration (LEGACY - frontend no longer uses Firebase directly)
# These values are kept only as historical placeholders. They are not read
# by the current codebase and can be safely ignored for new setups.
REACT_APP_FIREBASE_API_KEY=your_production_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# Backend API URL
REACT_APP_API_URL=https://your-backend-api.com

# App Configuration
REACT_APP_NAME=LMS
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# Analytics (Optional)
REACT_APP_ANALYTICS_ID=your_analytics_id
